{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/server.ts",
      "sourceMaps": true,
      "preLaunchTask": "npm: build",
      "trace": "all",
      "outFiles": [
        "${workspaceFolder}/build/**/*.js"
      ]
    }
  ]
}