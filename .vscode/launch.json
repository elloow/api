{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Docker launch debugging",
      "port": 4009,
      "restart": true,
      "localRoot": "${workspaceFolder}",
      "remoteRoot": "/home/node/app",
      "outFiles": [
        "${workspaceFolder}/build/**/*.js"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ],
      "internalConsoleOptions": "neverOpen",
      "sourceMaps": true
    },
  ]
}
